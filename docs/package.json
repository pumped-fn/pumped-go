{
  "name": "pumped-fn-docs",
  "version": "1.0.0",
  "description": "Nextra documentation site for pumped-fn library",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "next dev --turbopack",
    "build:next": "next build",
    "build:search": "pagefind --site .next/server/app --output-path out/_pagefind",
