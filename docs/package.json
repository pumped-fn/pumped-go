    "mdx": "^0.3.1",
    "next": "^15.3.5",
    "nextra": "^4.2.17",
    "nextra-theme-docs": "^4.2.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.4",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "pagefind": "^1.3.0",
    "typescript": "^5.8.3"
  },
  "type": "module"
}